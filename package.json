{
  "name": "travel-web",
  "version": "1.0.0",
  "private": false,
  "description": "A travel-themed web project using Tailwind CSS",
  "author": "Matins",
  "scripts": {
    "dev": "concurrently \"npm run watch:css\" \"npm run copy:assets:dev\"",
    "watch:css": "tailwindcss -i ./src/input.css -o ./dist/css/output.css --watch",
    "build": "node build.js",
    "copy:assets:dev": "node build.js"
  },
  "dependencies": {
    "tailwindcss": "^3.3.0",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }}
}
}
